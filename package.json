{
  "name": "my-electron-app",
  "version": "1.0.0",
  "description": "Hello World!",
  "main": "main.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "build": "electron-builder --win --x64",
    "server-react": "cross-env NODE_ENV=development && node ./server/bin/www",
    "dev-server-react": "cross-env NODE_ENV=development && nodemon ./server/bin/www",
    "prod-server-ract": "cross-env NODE_ENV=production pm2 start ./server/bin/www",
    "start-server": "node ./client/scripts/start",
    "build-react": "node ./client/scripts/build",
    "test": "node ./client/scripts/test"
  },
  "author": "LHC",
  "license": "MIT",
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.61",
    "@electron-forge/maker-deb": "^6.0.0-beta.61",
    "@electron-forge/maker-rpm": "^6.0.0-beta.61",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.61",
    "@electron-forge/maker-zip": "^6.0.0-beta.61",
    "electron": "15.3.4",
    "electron-builder": "^22.9.1",
    "@babel/core": "^7.2.2",
    "@babel/plugin-transform-react-jsx": "^7.2.0",
    "autoprefixer": "^7.2.6",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^8.0.5",
    "babel-plugin-import": "^1.13.0",
    "babel-plugin-named-asset-import": "^0.3.6",
    "babel-preset-react-app": "^7.0.2",
    "bfj": "^6.1.1",
    "caniuse-lite": "^1.0.30001192",
    "case-sensitive-paths-webpack-plugin": "^2.1.1",
    "chokidar": "^3.4.0",
    "cross-env": "^6.0.3",
    "css-loader": "^1.0.1",
    "eslint": "^5.6.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-react-app": "^2.1.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-flowtype": "^2.46.3",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "file-loader": "^2.0.0",
    "fork-ts-checker-webpack-plugin-alt": "^0.4.14",
    "fs-extra": "^7.0.1",
    "html-webpack-plugin": "^4.3.0",
    "jest": "^26.2.2",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "mini-css-extract-plugin": "^0.10.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "pnp-webpack-plugin": "^1.1.0",
    "postcss-flexbugs-fixes": "^3.2.0",
    "postcss-loader": "^2.1.1",
    "postcss-preset-env": "^6.3.1",
    "postcss-safe-parser": "^4.0.1",
    "rc-util": "^4.20.0",
    "react-dev-utils": "^7.0.1",
    "sass-loader": "7.1.0",
    "style-loader": "^0.19.0",
    "sw-precache-webpack-plugin": "^0.11.4",
    "terser-webpack-plugin": "^1.1.0",
    "url-loader": "^0.6.2",
    "webpack": "^4.44.1",
    "webpack-dev-server": "^3.11.0",
    "webpack-manifest-plugin": "^2.0.0-rc.2",
    "workbox-webpack-plugin": "^3.6.3"
  },
  "dependencies": {
    "@ant-design/icons": "^4.2.2",
    "@material-ui/core": "^4.8.1",
    "antd": "^4.5.4",
    "big-integer": "^1.6.25",
    "body-parser": "~1.18.2",
    "cookie-parser": "~1.4.3",
    "crypto": "^1.0.1",
    "debug": "~2.6.9",
    "electron-squirrel-startup": "^1.0.0",
    "express": "^4.17.1",
    "express-useragent": "^1.0.12",
    "morgan": "~1.9.0",
    "node-fetch": "^2.6.1",
    "pac-proxy-agent": "^4.1.0",
    "querystring": "^0.2.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-icons": "^3.9.0",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.5",
    "request": "^2.88.2",
    "simple-netease-cloud-music": "^0.5.0",
    "socket.io": "^4.4.1"
  },
  "build": {
    "productName": "我的音乐盒",
    "nsis": {
      "oneClick": false,
      "allowElevation": true,
      "allowToChangeInstallationDirectory": true,
      "installerIcon": "./resources/icon1.ico",
      "uninstallerIcon": "./resources/icon1.ico",
      "installerHeaderIcon": "./resources/icon1.ico",
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "shortcutName": "我的音乐盒快捷方式"
    },
    "win": {
      "icon": "resources/icon1.ico"
    }
  },
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "my_electron_app"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  },
  "babel": {
    "presets": [
      "react-app"
    ],
    "plugins": [
      [
        "import",
        {
          "libraryName": "antd",
          "style": true
        }
      ]
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "homepage": "http://localhost:8081/"
}
